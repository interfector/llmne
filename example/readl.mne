#!/usr/local/bin/llmne -x

_START:

STPUSH PUSHED
CALL READL

STPUSH PUSHED+1
CALL PRINT

EXIT 0

# READL function starts

READL:

SUBSP 1

DISPLAY CHAR
STPOP LOOP
LOAD LOOP
ADD GAP
SAVE LOOP+1

LOOP:
READ STRING
LOAD STRING

CMP EOT
JZ END

INC LOOP
INC LOOP+1

JMP LOOP

END:

ADDSP 1

DISPLAY INT
RET

GAP:
0300 # difference between 'PUSH 00' and 'READ 00'

EOT:
0010

# READL function ends

# PRINT function starts

PRINT:

SUBSP 1

DISPLAY CHAR
STPOP PLOOP

PLOOP:

LOAD PLOOP
CMP NULL
JZ ENDP

SAVE $$+4
WRITE $$+3

INC PLOOP

JMP PLOOP
NOP

NULL:
0000

ENDP:
ADDSP 1 
DISPLAY INT
RET

# PRINT function ends

PUSHED:
READ STRING
LOAD STRING

STRING:
0000
