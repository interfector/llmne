#!/usr/local/bin/llmne -x
# a fuggy interpreter written in llmne

READER:

DISPLAY CHAR
WRITE PROMPT
WRITE PROMPT+1
WRITE PROMPT+2

DISPLAY INT

LOOP:
READ INSTR
LOAD INSTR
CMP TERM
JZ INSTR
# Increment the instruction reader pointer
INC LOOP
INC LOOP+1
JMP READER

TERM:
9999

PROMPT:
0063 '?'
0062 '>'
0032 ' '

INSTR:
0000
0000
0000
0000
0000
0000
0000
0000
0000
0000
0000
0000
0000
0000
0000

END:
3300
