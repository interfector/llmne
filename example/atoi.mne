#!/usr/local/bin/llmne -x

MAIN:

STPUSH TOPUSH
CALL READL

ALOOP:
LOAD STRING
CMP NULL
JZ _END

CMP LIMIT
JM ALOOP_END

CMP LIMIT+1
JG ALOOP_END

#WRITE $$

SUB LIMIT+2
SAVE NUMBER+1

LOAD NUMBER
MUL NUMBER+2
ADD NUMBER+1
SAVE NUMBER

ALOOP_END:

INC ALOOP

JMP ALOOP

_END:
WRITE NUMBER
EXIT 0

