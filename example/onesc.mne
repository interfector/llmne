#!/usr/local/bin/llmne -x

_START:

DISPLAY CHAR

LOOP:
READ STRING
PUSH STRING
CMP EOF
JZ PRINT

PUSH STRING
SUB GAP
POP STRING

INC LOOP
INC LOOP+1
INC LOOP+4
INC LOOP+6

JMP LOOP

PRINT:
PUSH STRING
CMP EOF
JZ END

NOT STRING
ADD GAP
POP STRING
WRITE STRING

INC PRINT
INC PRINT+3
INC PRINT+5
INC PRINT+6

JMP PRINT

END:
WRITE EOF
EXIT 0

EOF:
10

GAP:
0048

STRING:
0000
0000
0000
0000
0000
